{
  "name": "libern/someline-starter-api-client",
  "description": "Rest API Client for Someline Starter",
  "keywords": [
    "rest",
    "api",
    "client",
    "someline-starter"
  ],
  "homepage": "https://github.com/libern/someline-starter-api-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Libern Lin",
      "email": "libernlin@gmail.com",
      "homepage": "https://www.someline.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "5.2.*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Libern\\Rest\\": "src/Libern/Rest"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Libern\\Rest\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
